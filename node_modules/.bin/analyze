#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/khallmark/Desktop/Code/neurotypic-ai/magus-mark/node_modules/.pnpm/vite-bundle-analyzer@1.2.3/node_modules/vite-bundle-analyzer/dist/node_modules:/Users/khallmark/Desktop/Code/neurotypic-ai/magus-mark/node_modules/.pnpm/vite-bundle-analyzer@1.2.3/node_modules/vite-bundle-analyzer/node_modules:/Users/khallmark/Desktop/Code/neurotypic-ai/magus-mark/node_modules/.pnpm/vite-bundle-analyzer@1.2.3/node_modules:/Users/khallmark/Desktop/Code/neurotypic-ai/magus-mark/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/khallmark/Desktop/Code/neurotypic-ai/magus-mark/node_modules/.pnpm/vite-bundle-analyzer@1.2.3/node_modules/vite-bundle-analyzer/dist/node_modules:/Users/khallmark/Desktop/Code/neurotypic-ai/magus-mark/node_modules/.pnpm/vite-bundle-analyzer@1.2.3/node_modules/vite-bundle-analyzer/node_modules:/Users/khallmark/Desktop/Code/neurotypic-ai/magus-mark/node_modules/.pnpm/vite-bundle-analyzer@1.2.3/node_modules:/Users/khallmark/Desktop/Code/neurotypic-ai/magus-mark/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite-bundle-analyzer/dist/bin.js" "$@"
else
  exec node  "$basedir/../vite-bundle-analyzer/dist/bin.js" "$@"
fi
